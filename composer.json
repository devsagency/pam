{
  "name": "pam.php",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/philippebeck/pam.php"
  },
  "type": "framework",
  "description": "Php Approachable Microframework",
  "autoload": {
